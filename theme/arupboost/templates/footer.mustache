{{!
    Page footer.
}}
<footer id="arup-page-footer" class="py-3 bg-gray010">
    <div class="container-fluid">
        <div class="position-relative py-1">
            <hr>
            <h3 class="position-absolute bg-gray010 px-3" style="top: 2px">{{#str}} helpsupport, theme_arupboost {{/str}}</h3>
        </div>
        {{> theme_arupboost/footer_content }}
        <div id="course-footer">{{{ output.course_footer }}}</div>

        <div class="d-flex systemlinks small muted align-items-center px-3">
            <span class="copyright mr-3">
                 {{{output.copyright}}}
            </span>
            {{# output.page_doc_link }}
                {{{ output.page_doc_link }}}
            {{/ output.page_doc_link }}

             <div class="tool_usertours-resettourcontainer"></div>
        </div>
        {{{ output.standard_end_of_body_html }}}
        {{{ output.standard_footer_html }}}
    </div>
</footer>