<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/tapsenrol/db" VERSION="20130128" COMMENT="XMLDB file for Moodle mod_tapsenrol"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="tapsenrol" COMMENT="Defines tapsenrol">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="intro" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="internalworkflowid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="completionenrolment" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
        <FIELD NAME="instanceok" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
        <FIELD NAME="completionattended" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
        <KEY NAME="internalworkflowid" TYPE="foreign" FIELDS="internalworkflowid" REFTABLE="tapsenrol_iw" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="tapsenrol_region" COMMENT="tapsenrol to region mapping">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
        <FIELD NAME="tapsenrolid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
        <FIELD NAME="regionid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="tapsenrol_completion" COMMENT="Defines tapsenrol_completion">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="tapsenrolid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="completed" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="tapsenrol_iw" COMMENT="Defines tapsenrol_iw">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="regionid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="0"/>
        <FIELD NAME="enroltype" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" DEFAULT="enrol"/>
        <FIELD NAME="approvalrequired" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="1"/>
        <FIELD NAME="approvalreminder" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="604800"/>
        <FIELD NAME="cancelafter" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="1209600"/>
        <FIELD NAME="cancelbefore" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="86400"/>
        <FIELD NAME="closeenrolment" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="172800"/>
        <FIELD NAME="firstreminder" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="1209600"/>
        <FIELD NAME="secondreminder" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="259200"/>
        <FIELD NAME="noreminder" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" DEFAULT="172800"/>
        <FIELD NAME="fromfirstname" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="fromlastname" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="fromemail" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="sponsors" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="rejectioncomments" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0"/>
        <FIELD NAME="cancelcomments" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0"/>
        <FIELD NAME="enrolinfo" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="approveinfo" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="rejectinfo" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="eitherinfo" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="cancelinfo" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="locked" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0"/>
        <FIELD NAME="emailsoff" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="tapsenrol_iw_email" COMMENT="Defines tapsenrol_iw_email">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="sortorder" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="sendtype" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="email" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="subject" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="body" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="html" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
        <KEY NAME="email" TYPE="unique" FIELDS="email"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="tapsenrol_iw_email_custom" COMMENT="Defines tapsenrol_iw_email_custom">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="internalworkflowid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="coursemoduleid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="emailid" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="subject" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="body" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="html" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
        <KEY NAME="emailid" TYPE="foreign" FIELDS="emailid" REFTABLE="tapsenrol_iw_email" REFFIELDS="id" />
        <KEY NAME="iwid_cmid_emailid" TYPE="unique" FIELDS="internalworkflowid, coursemoduleid, emailid" PREV="emailid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="tapsenrol_iw_tracking" COMMENT="Defines tapsenrol_iw_tracking">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="enrolmentid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="sponsoremail" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="sponsorfirstname" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="sponsorlastname" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="requestcomments" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="approved" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="approvecomments" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="cancelcomments" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timeenrolled" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timeapproved" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timecancelled" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="remindersent" TYPE="int" LENGTH="2" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="moved" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
        <KEY NAME="enrolmentid" TYPE="foreign-unique" FIELDS="enrolmentid" REFTABLE="tapsenrol_class_enrolments" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="tapsenrol_iw_declaration" COMMENT="Defines tapsenrol_iw_declaration">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="internalworkflowid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="declarationid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="declaration" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="iwid_decid" TYPE="unique" FIELDS="internalworkflowid, declarationid"/>
        <KEY NAME="internalworkflowid" TYPE="foreign" FIELDS="internalworkflowid" REFTABLE="tapsenrol_iw" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
