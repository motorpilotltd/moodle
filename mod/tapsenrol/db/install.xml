        <FIELD NAME="body" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="html" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
        <KEY NAME="emailid" TYPE="foreign" FIELDS="emailid" REFTABLE="tapsenrol_iw_email" REFFIELDS="id" />
        <KEY NAME="iwid_cmid_emailid" TYPE="unique" FIELDS="internalworkflowid, coursemoduleid, emailid" PREV="emailid"/>
