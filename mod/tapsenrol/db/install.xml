    <TABLE NAME="tapsenrol_iw_declaration" COMMENT="Defines tapsenrol_iw_declaration">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
