<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/learningrecordstore/db" VERSION="20170511" COMMENT="XMLDB file for Moodle local/taps"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_learningrecordstore" COMMENT="Cache for TAPS enrolments">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="provider" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="healthandsafetycategory" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="providerid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="providername" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="staffid" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="duration" TYPE="float" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="durationunits" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="completiontime" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="certificateno" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="starttime" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="endtime" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>



        <FIELD NAME="classid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="coursename" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="location" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="classtype" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="classcategory" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="durationunitscode" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseobjectives" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="expirydate" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="bookingstatus" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="personid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>

        <FIELD NAME="classcost" TYPE="number" LENGTH="20" NOTNULL="false" SEQUENCE="false" DECIMALS="2"/>
        <FIELD NAME="classcostcurrency" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="learningdesccont1" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="learningdesccont2" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timezone" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="usedtimezone" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="UTC" SEQUENCE="false"/>
        <FIELD NAME="pricebasis" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="currencycode" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="price" TYPE="number" LENGTH="20" NOTNULL="false" SEQUENCE="false" DECIMALS="2"/>
        <FIELD NAME="bookingplaceddate" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="active" TYPE="int" LENGTH="4" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
        <FIELD NAME="archived" TYPE="int" LENGTH="4" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="locked" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="providerid" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="staffid" UNIQUE="false" FIELDS="staffid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>