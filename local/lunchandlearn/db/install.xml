<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/lunchandlearn/db" VERSION="2014050100" COMMENT="XMLDB file for local_regions tables"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>

     <TABLE NAME="local_lunchandlearn" COMMENT="">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="eventid"/>
        <FIELD NAME="eventid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="regionid"/>
        <FIELD NAME="regionid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="eventid" NEXT="office"/>
        <FIELD NAME="office" TYPE="char" LENGTH="80" NOTNULL="false" SEQUENCE="false" PREVIOUS="regionid" NEXT="room"/>
        <FIELD NAME="room" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="office" NEXT="categoryid"/>
        <FIELD NAME="categoryid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="room" NEXT="tutorid"/>
        <FIELD NAME="tutorid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" PREVIOUS="categoryid" NEXT="supplier"/>
        <FIELD NAME="supplier" TYPE="char" LENGTH="80" NOTNULL="false" SEQUENCE="false" PREVIOUS="tutorid" NEXT="summary"/>
        <FIELD NAME="summary" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="supplier" NEXT="joindetail" />
        <FIELD NAME="joindetail" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="summary" NEXT="description" />
        <FIELD NAME="description" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="categoryid" NEXT="sessioninfo" />
        <FIELD NAME="sessioninfo" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="description" NEXT="availableonline" />
        <FIELD NAME="availableonline" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0" PREVIOUS="sessioninfo" NEXT="availableinperson" />
        <FIELD NAME="availableinperson" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0" PREVIOUS="availableonline" NEXT="capacity" />
        <FIELD NAME="capacity" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" PREVIOUS="description" NEXT="onlinecapacity" />
        <FIELD NAME="onlinecapacity" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" PREVIOUS="capacity" NEXT="overbookinperson" />
        <FIELD NAME="overbookinperson" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0" PREVIOUS="onlinecapacity" NEXT="overbookonline" />
        <FIELD NAME="overbookonline" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0" PREVIOUS="overbookinperson" NEXT="recording" />
        <FIELD NAME="recording" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="overbookonline" NEXT="timezone" />
        <FIELD NAME="timezone" TYPE="char" LENGTH="40" NOTNULL="false" SEQUENCE="false" PREVIOUS="recording" NEXT="locked" />
        <FIELD NAME="locked" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0" PREVIOUS="timezone" NEXT="cancelled" />
        <FIELD NAME="cancelled" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="0" PREVIOUS="locked" />
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
     </TABLE>
     <TABLE NAME="local_lunchlearn_attendees" COMMENT="">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="lunchandlearnid"/>
        <FIELD NAME="lunchandlearnid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="lunchandlearnid" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="5" NOTNULL="true" SEQUENCE="false" PREVIOUS="userid" NEXT="notes" />
        <FIELD NAME="notes" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="status" NEXT="requirements" />
        <FIELD NAME="requirements" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="notes" NEXT="inperson" />
        <FIELD NAME="inperson" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" PREVIOUS="requirements" />
        <FIELD NAME="usersignedup" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" PREVIOUS="inperson" NEXT="timesignup" />
        <FIELD NAME="timesignup" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" PREVIOUS="usersignedup" NEXT="userupdated" />
        <FIELD NAME="userupdated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" PREVIOUS="timesignup" NEXT="timeupdated" />
        <FIELD NAME="timeupdated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" PREVIOUS="userupdated" />
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
     </TABLE>
  </TABLES>
</XMLDB>
