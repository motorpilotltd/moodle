<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/taps/db" VERSION="20170511" COMMENT="XMLDB file for Moodle local/taps"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_taps_course" COMMENT="Cache for TAPS courses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="coursecode" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="coursename" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="startdate" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="enddate" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseregion" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="coursedescription" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseobjectives" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseaudience" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="globallearningstandards" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="onelinedescription" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="businessneed" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="accreditationgivendate" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="tapsurllink" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="keywords" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="duration" TYPE="float" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="durationunits" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="durationunitscode" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="sponsorname" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseadminempno" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseadminempname" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="maximumattendees" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="minimumattendees" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="futurereviewdate" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="jobnumber" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="activecourse" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="usedtimezone" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="UTC" SEQUENCE="false"/>
        <FIELD NAME="archived" TYPE="int" LENGTH="4" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="courseid" TYPE="unique" FIELDS="courseid"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="local_taps_course_category" COMMENT="Cache for TAPS course categories">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="categoryhierarchy" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="primaryflag" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>