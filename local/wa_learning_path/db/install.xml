<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="local/wa_agency/db" VERSION="20200228" COMMENT="XMLDB file for Moodle local/wa_agency" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="wa_learning_path" COMMENT="WA Learning paths">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="category" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="summary" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="introduction" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="matrix" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="format" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="keywords" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="status" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="preview" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="parent" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="wa_learning_path_region" COMMENT="WA Learning paths regions">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="learningpathid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="regionid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="wa_lp_inde_learningpathid" UNIQUE="false" FIELDS="learningpathid" COMMENT="The learning path regions"/>
                <INDEX NAME="wa_lp_inde_regionid" UNIQUE="false" FIELDS="regionid" COMMENT="The learning path regions"/>
            </INDEXES>
        </TABLE>
        <TABLE NAME="wa_learning_path_subscribe" COMMENT="WA Learning path subscription">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="learningpathid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="status" TYPE="int" LENGTH="3" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="timeupdated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="wa_lp_index_learningpathid" UNIQUE="false" FIELDS="learningpathid" COMMENT="The learning path this subscription instance belongs to."/>
                <INDEX NAME="wa_lp_index_userid" UNIQUE="false" FIELDS="userid" COMMENT="The user this subscription instance belongs to."/>
                <INDEX NAME="wa_lp_index_status" UNIQUE="false" FIELDS="status" COMMENT="The status of this instance."/>
            </INDEXES>
        </TABLE>
        <TABLE NAME="wa_learning_path_activity" COMMENT="WA Activity">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="type" TYPE="char" LENGTH="50" NOTNULL="true" DEFAULT="video" SEQUENCE="false"/>
                <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="enablecdp" TYPE="int" LENGTH="3" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="learningmethod" TYPE="char" LENGTH="255" NOTNULL="false" DEFAULT="SELF-LED" SEQUENCE="false"/>
                <FIELD NAME="provider" TYPE="char" LENGTH="255" NOTNULL="false" DEFAULT="" SEQUENCE="false"/>
                <FIELD NAME="duration" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="unit" TYPE="char" LENGTH="20" NOTNULL="false" DEFAULT="MIN" SEQUENCE="false"/>
                <FIELD NAME="subject" TYPE="char" LENGTH="255" NOTNULL="false" DEFAULT="PD" SEQUENCE="false"/>
                <FIELD NAME="learningdescription" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="completiondate" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="idlearningpath" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="timeupdated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="wa_lpa_index_type" UNIQUE="false" FIELDS="type"/>
                <INDEX NAME="wa_lp_index_lp" UNIQUE="false" FIELDS="idlearningpath" COMMENT="Learning path"/>
            </INDEXES>
        </TABLE>
        <TABLE NAME="wa_learning_path_act_region" COMMENT="WA Activities regions">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="activityid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="regionid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="wa_lpar_inde_activityid" UNIQUE="false" FIELDS="activityid" COMMENT="The Activity"/>
                <INDEX NAME="wa_lpar_inde_regionid" UNIQUE="false" FIELDS="regionid" COMMENT="The region"/>
            </INDEXES>
        </TABLE>
        <TABLE NAME="wa_learning_path_a_com" COMMENT="WA activity completions">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="activityid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="timeupdated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="wa_lpac_index_activityid" UNIQUE="false" FIELDS="activityid" COMMENT="The activity this completion instance belongs to."/>
                <INDEX NAME="wa_lpac_index_userid" UNIQUE="false" FIELDS="userid" COMMENT="The user this completion instance belongs to."/>
            </INDEXES>
        </TABLE>
        <TABLE NAME="wa_learning_path_role" COMMENT="WA Roles for new learning pathways">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="128" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="visible" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="learningpathid" TYPE="int" NOTNULL="false" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="wa_learning_path_role_act" COMMENT="WA Learning Path - Learning Journey activity settings for role">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="roleid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="activityid" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="itemid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="type" TYPE="char" LENGTH="32" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="visible" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="overridedescription" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="overrideere" TYPE="char" LENGTH="32" NOTNULL="false" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>