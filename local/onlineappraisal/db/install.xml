  <TABLES>
    <TABLE NAME="local_appraisal_appraisal" COMMENT="Default comment for local_onlineappraisal, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="appraisee_userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="The ID number of the Moodle user assigned to the appraisal as an appraisee"/>
        <FIELD NAME="appraiser_userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="The ID number of the Moodle user assigned to the appraisal as an appraiser"/>
        <FIELD NAME="signoff_userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="The ID number of the Moodle user assigned to the appraisal as the signoff user. This is typically the group leader."/>
        <FIELD NAME="groupleader_userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="The ID number of the Moodle user assigned to the appraisal as the groupleader user."/>
        <FIELD NAME="previous_appraisalid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="The ID number of the previous appraisal. This field is optional."/>
        <FIELD NAME="statusid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Reference to the appraisal status."/>
        <FIELD NAME="permissionsid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Reference to the appraisal status. Determines permissions as can be different from status."/>
        <FIELD NAME="created_date" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Date on which the appraisal was initiated by the group administrator."/>
        <FIELD NAME="held_date" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Date appraisal face to face was held."/>
        <FIELD NAME="due_date" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Due date of the appraisal, as set by the group administrator."/>
        <FIELD NAME="job_title" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Text field, added by appraisee."/>
        <FIELD NAME="operational_job_title" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Text field, added by appraisee."/>
        <FIELD NAME="grade" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Text field, added by appraisee."/>
        <FIELD NAME="teamperformance_comments" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="six_month_review" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Text field, added by appraiser."/>
        <FIELD NAME="six_month_review_date" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Date six month review was completed."/>
        <FIELD NAME="face_to_face_held" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT="True/false option."/>
        <FIELD NAME="completed_date" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" COMMENT="Date appraisal was completed."/>
        <FIELD NAME="archived" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="True/false option."/>
        <FIELD NAME="deleted" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="True/false option."/>
        <FIELD NAME="status_history" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="modified_date" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="legacy" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="True/false option."/>
        <FIELD NAME="successionplan" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="True/false option."/>
        <FIELD NAME="leaderplan" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="True/false option."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
