<div class="modal fade" id="oa-print-confirm-modal" tabindex="-1" role="dialog" aria-labelledby="oa-print-confirm-modal-label">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="{{#str}}modal:printconfirm:cancel, local_onlineappraisal{{/str}}"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="oa-print-confirm-modal-label">{{#str}}modal:printconfirm:title, local_onlineappraisal{{/str}}</h4>
            </div>
            <div class="modal-body">
                <p>{{#str}}modal:printconfirm:content, local_onlineappraisal{{/str}}</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">{{#str}}modal:printconfirm:cancel, local_onlineappraisal{{/str}}</button>
                <a id="oa-print-confirm-modal-continue" href="#" class="btn btn-primary">{{#str}}modal:printconfirm:continue, local_onlineappraisal{{/str}}</a>
            </div>
        </div>
    </div>
</div>