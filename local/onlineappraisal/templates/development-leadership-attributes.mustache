<table class="table oa-table-leadership-attributes text-center hidden" data-type="{{type}}">
    <thead>
        <tr>
        {{#headings}}
            <th scope="col" data-column="{{.}}">{{.}}</th>
        {{/headings}}
        </tr>
    </thead>
    <tbody>
    {{#rows}}
        <tr>
        {{#cells}}
            <td><button class="btn btn-default" data-option="{{option}}" data-selected="false" {{disabled}}>{{name}}</button>{{!
            }}<i role="button"
                 class="fa fa-info-circle fa-lg fa-fw"
                 data-toggle="popover" data-content="{{info}}"" data-html="true" data-container="body"></i></td>
        {{/cells}}
        </tr>
    {{/rows}}
    </tbody>
</table>